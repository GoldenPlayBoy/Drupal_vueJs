{
  "name": "contenta_vue_nuxt",
  "version": "2.0.0",
  "description": "Nuxt.js client for Contenta CMS",
  "author": "yann <yann@yineo.fr>",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "jsonapi-parse": "^2.0.0",
    "nuxt": "^2.2.0",
    "vue-lazyload": "1.0.6"
  },
  "scripts": {
    "dev": "node_modules/.bin/nuxt",
    "build": "node_modules/.bin/nuxt build",
    "analyse": "node_modules/.bin/nuxt build --analyse",
    "start": "node_modules/.bin/nuxt start",
    "generate": "node_modules/.bin/nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "karma start test/unit/karma.conf.js",
    "precommit": "npm run test",
    "prettier": "prettier --write './**/*.{js,vue}' && eslint './**/*.{js,vue}Difficulty.vue' --fix"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^4.7.1",
    "jasmine-core": "^2.6.4",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "prettier": "^1.14.3",
    "push-dir": "^0.4.1"
  }
}
